&pwm2 {
	status = "okay";
};

&u2phy0 {
	status = "okay";
	extcon = <&fusb0>;

	u2phy0_host: host-port {
		status = "okay";
		phy-supply = <&vcc5v0_host>;
	};

    u2phy0_otg_port: host-otg {
		status = "okay";
	};
};

&u2phy1 {
	status = "okay";

	u2phy1_host: host-port {
		status = "okay";
		phy-supply = <&vcc5v0_host>;
	};

    u2phy1_otg_port: host-otg {
		status = "okay";
	};
};

&tcphy0 {
	extcon = <&fusb0>;
	status = "okay";
};

&tcphy1 {
	status = "okay";
};

&spdif {
	pinctrl-0 = <&spdif_bus_1>;
	status = "okay";
	#sound-dai-cells = <0>;
};

