&hdmi {
	avdd-0v9-supply = <&vcca0v9_hdmi>;
	avdd-1v8-supply = <&vcca1v8_hdmi>;
	ddc-i2c-bus = <&i2c3>;
	pinctrl-names = "default";
	pinctrl-0 = <&hdmi_cec>;
	status = "okay";
